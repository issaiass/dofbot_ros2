    <!-- joint concatenation -->
    <xacro:fixed_joint name="base_joint" parent="base_footprint" child="base_link" xyz="0.0 0.0 0.076" rpy="0 0 0"/>
    <xacro:revolute_joint name="arm_joint1" parent="base_link" child="arm_link1" xyz="0 0 0.07" rpy="0 0 ${pi/2}" axisZ="-1" lower="-${pi/2}" upper="${pi/2}"/>
    <xacro:revolute_joint name="arm_joint2" parent="arm_link1" child="arm_link2" xyz="0 0 0.0405" rpy="-${pi/2} 0 0" axisZ="-1" lower="-${pi/2}" upper="${pi/2}"/>
    <xacro:revolute_joint name="arm_joint3" parent="arm_link2" child="arm_link3" xyz="0 -0.0829 0" rpy="0 0 0" axisZ="-1" lower="-${pi/2}" upper="${pi/2}"/>
    <xacro:revolute_joint name="arm_joint4" parent="arm_link3" child="arm_link4" xyz="0 -0.0829 0" rpy="0 0 0" axisZ="-1" lower="-${pi/2}" upper="${pi/2}"/>
    <xacro:revolute_joint name="arm_joint5" parent="arm_link4" child="arm_link5" xyz="-0.00215 -0.17455 0" rpy="${pi/2} 0 0" axisZ="1" lower="-${pi/2}" upper="${pi}"/>
    <xacro:revolute_joint name="grip_joint" parent="arm_link5" child="rlink1" xyz="-0.0035 -0.012625 -0.0685" rpy="0 -${pi/2} 0" axisZ="1" lower="-${pi/2}" upper="0"/>
    <!-- END joint concatenation -->

    <!-- gripper joints -->
    <xacro:continuous_joint name="rlink_joint2" parent="rlink1" child="rlink2" xyz="0.03 -0.00075429 0" rpy="0 0 0" mimic="-1"/>
    <xacro:continuous_joint name="rlink_joint3" parent="arm_link5" child="rlink3" xyz="-0.0035 -0.0045 -0.0505" rpy="0 -${pi/2} 0" mimic="1"/>
    <xacro:continuous_joint name="llink_joint1" parent="arm_link5" child="llink1" xyz="-0.0035 0.012375 -0.0685" rpy="0 -${pi/2} 0" mimic="-1"/>
    <xacro:continuous_joint name="llink_joint2" parent="llink1" child="llink2" xyz="0.03 0.00065104 0" rpy="0 0 0" mimic="1"/>
    <xacro:continuous_joint name="llink_joint3" parent="arm_link5" child="llink3" xyz="-0.0035 0.0045 -0.0505" rpy="0 -${pi/2} 0" mimic="-1"/>
    <!-- END gripper joints -->

    <!--rgb camera joint -->
    <xacro:fixed_joint name="mono_joint" parent="arm_link4" child="mono_link" xyz="-0.0481 -0.05145 -0.0022" rpy="${pi/2} 0 0"/>
    <!-- rgbd camera joint -->
    <xacro:fixed_joint name="camera_joint" parent="base_link" child="camera_link" xyz="0 -0.2 0.41955" rpy="0 ${pi/4} ${pi/2}"/>
